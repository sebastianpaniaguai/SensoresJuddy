<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.juddyreina.sensores">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_sensor"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".magnetic" />
        <activity android:name=".orientation" />
        <activity android:name=".proximity" />
        <activity android:name=".light" />
        <activity android:name=".rotation" />
        <activity android:name=".accelerometer" />
        <activity android:name=".sensores"></activity>
    </application>

</manifest>
